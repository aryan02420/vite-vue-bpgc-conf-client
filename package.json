{
  "name": "vite-vue-bpgc-conf-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "build:serve": "serve -s dist/",
    "deploy": "npm run build -- --base=/vite-vue-bpgc-conf-client/ && cp dist/index.html dist/404.html && push-dir --dir=dist --branch=gh-pages"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.15.0",
    "axios": "^0.21.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.3.0",
    "push-dir": "^0.4.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3",
    "vite": "^2.3.3",
    "vue-tsc": "^0.0.24"
  }
}
